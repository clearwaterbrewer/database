DROP TABLE IF EXISTS classtypes;
CREATE TABLE classtypes (
    Class varchar(1) NOT NULL,
    ClassType varchar(30) NOT NULL,
    PRIMARY KEY (Class),
    UNIQUE (ClassType)
);

