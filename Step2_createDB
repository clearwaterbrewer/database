DROP TABLE IF EXISTS Batches;
CREATE TABLE Batches (
    BatchNum INT UNSIGNED NOT NULL,
    BatchName varchar(30) NOT NULL,
    Class varchar(30),
    SourceProduct varchar(30),
    SourceIngredient varchar(30),
    PRIMARY KEY (BatchNum)
);

